defaults
auth on
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt

account jon@eyl.io
host eyl.io
port 587
from jon@eyl.io
user jon
passwordeval pass show eyl

account jeyolfso@uwaterloo.ca
host mailservices.uwaterloo.ca
port 587
from jeyolfso@uwaterloo.ca
user jeyolfso
passwordeval pass show uwaterloo/watiam

account default : jon@eyl.io
